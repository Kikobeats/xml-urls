{
  "name": "xml-urls",
  "description": "Get all urls from a Feed/Atom/RSS/Sitemap xml markup.",
  "homepage": "https://nicedoc.io/Kikobeats/xml-urls",
  "version": "2.1.28",
  "main": "index.js",
  "author": {
    "email": "josefrancisco.verdu@gmail.com",
    "name": "Kiko Beats",
    "url": "https://kikobeats.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kikobeats/xml-urls.git"
  },
  "bugs": {
    "url": "https://github.com/Kikobeats/xml-urls/issues"
  },
  "keywords": [
    "sitemap",
    "url",
    "urls",
    "xml",
    "xml"
  ],
  "dependencies": {
    "@metascraper/helpers": "~5.26.0",
    "aigle": "~1.14.1",
    "cheerio": "~1.0.0-rc.10",
    "html-get": "~2.9.11",
    "lodash": "~4.17.21",
    "matcher": "~4.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "latest",
    "@commitlint/config-conventional": "latest",
    "ava": "latest",
    "browserless": "latest",
    "c8": "latest",
    "ci-publish": "latest",
    "conventional-github-releaser": "latest",
    "exit-hook": "2",
    "finepack": "latest",
    "git-authors-cli": "latest",
    "nano-staged": "latest",
    "npm-check-updates": "latest",
    "prettier-standard": "latest",
    "puppeteer": "latest",
    "simple-git-hooks": "latest",
    "standard": "latest",
    "standard-markdown": "latest",
    "standard-version": "latest"
  },
  "engines": {
    "node": ">= 6"
  },
  "files": [
    "index.js"
  ],
  "scripts": {
    "clean": "rm -rf node_modules",
    "contributors": "(git-authors-cli && finepack && git add package.json && git commit -m 'build: contributors' --no-verify) || true",
    "lint": "standard-markdown README.md && standard",
    "postrelease": "npm run release:tags && npm run release:github && (ci-publish || npm publish --access=public)",
    "prerelease": "npm run update:check && npm run contributors",
    "pretest": "npm run lint",
    "pretty": "prettier-standard index.js {core,test,bin}/**/*.js --single-quote",
    "release": "standard-version -a",
    "release:github": "conventional-github-releaser -p angular",
    "release:tags": "git push --follow-tags origin HEAD:master",
    "test": "c8 ava",
    "update": "ncu -u",
    "update:check": "ncu -- --error-level 2"
  },
  "license": "MIT",
  "ava": {
    "files": [
      "test/**/*",
      "!test/util.js"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "nano-staged": {
    "*.js": [
      "prettier-standard"
    ],
    "package.json": [
      "finepack --sort-ignore-object-at ava"
    ]
  },
  "simple-git-hooks": {
    "commit-msg": "npx commitlint --edit",
    "pre-commit": "npx nano-staged"
  }
}
